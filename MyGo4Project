package main

import "fmt"

func fibonacci(i int)(ret int){
	if i==0{
		return 0
	}
	if i==1{
		return 1

	}
	return fibonacci(i-1)+fibonacci(i-2)
}
func main() {
var i int
var limit int
fmt.Println("enter your limit Please :)")
fmt.Scanln(&limit)
fmt.Println("here your factorial :)")
for i=0;i<limit;i++{
	fmt.Printf("%d  ",fibonacci(i))
}
}
