public class checkelement {
    public static void main(String[]a){
        int [] num={1,2,4,5,6};
        int tofind=4;
        boolean found=false;
        for(int n:num){
            if(n==tofind){
                found=true;
                break;
            }
        }
        if (found)
            System.out.println(tofind + " is found  !!!!");
        else
            System.out.println(tofind + "is not found !!!!");
    }

}
