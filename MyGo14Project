package main

import "fmt"

func main() {
	var dayOfWeek int
	fmt.Println("enter your day of the Week :)")
	fmt.Scanln(&dayOfWeek)
	fmt.Println("Go Switch Case statement")
	switch dayOfWeek {
	case 1, 2, 3, 4, 5:
		fmt.Println("It's a Weekday.")
	case 6, 7:
		fmt.Println("Its Weekend.")
	default:
		fmt.Println("Invalid Day")
	}
	
}
