//inheritance In Swift Programming
class studentDetails{
var stname:String!
var mark1 :Int!
var mark2 :Int!
var mark3 :Int!
init(stname:String,mark1:Int,mark2:Int,mark3:Int){
self.stname=stname
self.mark1=mark1
self.mark2=mark2
self.mark3=mark3
}

}
let stname = "Dhiraj"
let mark1=90
let mark2=100
let mark3=89
print(stname)
print(mark1)
print(mark2)
print(mark3)


