//Another example of Inheritance
class StudDetails{
var mark1: Int
var mark2 :Int
init(stm1:Int,results stm2:Int){
mark1 = stm1
mark2 = stm2
}
func print1(){
print("Mark1 : \(mark1) ,Mark2 :\(mark2)")
}
}
class Display : StudDetails{
init(){
super.init(stm1 :99,results :100)
}
}
let marksobtained = Display()
marksobtained.print1()
