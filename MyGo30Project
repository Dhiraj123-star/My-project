package main
import "fmt"
func main() {
	names:=[4]string{  //array of strings
		"C#",
		"GO",
		"Scala",
		"Python",
	}
	fmt.Println(names)
	slice1 :=names[0:2] //slicing the array
	slice2:=names[1:3]
	fmt.Println(slice1,slice2)
	slice2[0]="Java"  //change the value of the slice where Go is positioned that is in slice2's 0 position there is Go, that place java is replaced
	fmt.Println(slice1,slice2)
	fmt.Println(names)  //in this names array  , Go Position is replaced by java

}
